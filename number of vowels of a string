#include <stdio.h>
#include <string.h>
void main()
{
int c=0,char a[100];
scanf("%[^/n]",a);
for (int i=0;a[i];i++)
{
if (a[i]=='a' || a[i]=='e' || a[i]=='o' || a[i]=='i' || a[i]=='u')
c++;
else if (a[i]=='A' || a[i]=='E' || a[i]=='O' || a[i]=='I' || a[i]=='U')
c++;
}
printf("Number of vowels:%d",c);
}
