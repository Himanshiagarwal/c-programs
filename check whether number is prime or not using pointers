#include<stdio.h>
void main()
{
	int i,m,*ptr;
	printf("enter numbers of elements of array:");
	scanf("%d",&m);
	printf("enter elements of array:");
	for(i=0;i<m;i++)
	{
		scanf("%d",ptr+i);
	}
	for(i=0;i<m;i++)
	{
		if(*(ptr+i)%2==0)
		printf("%d is prime number.\n",*(ptr+i));
		else
		printf("%d is not prime number.\n",*(ptr+i));
	}
}
