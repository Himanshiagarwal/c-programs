#include<stdio.h>
int main()
{
	int i,j,m,n;
	printf("enter number of rows and columns ");
	scanf("%d%d",&m,&n);
	int a[m][n];
	
	printf("elements in 2D array are ");
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		scanf("%d",&a[i][j]);
	}
	for(i=0;i<m;i++)
	{
		printf("\n");
		for(j=0;j<n;j++)
		{
			printf("%d\t",a[i][j]);
		}
	}
	return 0;
}
