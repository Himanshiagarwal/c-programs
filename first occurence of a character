#include<stdio.h>
#include<string.h>
int main()
{
	char s[100],ch;
	int i,c=0;
	printf("enter string ");
	scanf("%[^\n]",s);
	getchar();
	ch=getchar();
	for(i=0;i<strlen(s);i++)
	{
		if(s[i]==ch)
		{
			c++;
			break;
		}
	}
	if(c>0)
	printf("element is found at %d position ",i+1);
	else
	printf("oooppssss!!!! we dont find ur character in a given string");
	return 0;
}
